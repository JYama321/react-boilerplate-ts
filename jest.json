{
  "rootDir": "./",
  "testRegex": "\\.spec\\.tsx?$",
  "moduleFileExtensions": [
    "js",
    "jsx",
    "ts",
    "tsx"
  ],
  "setupFiles": [
    "<rootDir>/internals/test/polyfills.js",
    "<rootDir>/internals/test/setupTest.ts"
  ],
  "testURL": "http://localhost/",
  "transform": {
    "\\.(ts|tsx)$": "ts-jest"
  },
  "restoreMocks": true,
  "snapshotSerializers": ["enzyme-to-json"],
  "coveragePathIgnorePatterns": ["<rootDir>/node_modules/"]
}
